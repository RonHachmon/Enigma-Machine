                  <TableView fx:id="dictionaryTable" onMouseClicked="#selectedWord" prefHeight="100000.0" style="-fx-border-color: black;">
                    <columns>
                      <TableColumn fx:id="wordsColumn" editable="false" prefWidth="75.0" text="Dictionary words" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </ScrollPane>
         </center>
         <top>
            <TextField fx:id="searchBar" promptText="search" BorderPane.alignment="CENTER" />
