         </children>
      </HBox>
      <Label fx:id="percentageLabel" GridPane.columnIndex="3" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets top="16.0" />
         </GridPane.margin></Label>
      <VBox GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="142.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="145.0" text="Encryption duration in mil:" />
                  <Label fx:id="encryptTimeLabel" maxWidth="1.7976931348623157E308" prefHeight="17.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="142.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="165.0" text="Avg agent work time in mil:" />
                  <Label fx:id="averageTimeLabel" maxWidth="1.7976931348623157E308" prefHeight="17.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
